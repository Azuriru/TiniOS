{
    "name": "thunders-revenge",
    "version": "0.0.1",
    "description": "Electric Boogaloo: Thunder's Revenge",
    "productName": "Thunder's Revenge",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "build": "webpack --mode production",
        "watch": "webpack --watch --mode development",
        "dev": "concurrently \"npm:start\" \"npm:watch\" \"npm:open-browser\"",
        "open-browser": "sleep 5 && open-cli http://localhost:3000",
        "package-win": "electron-packager . --prune --overwrite --platform=win32 --arch=ia32 --icon=icons/heart.ico  --out=releases",
        "graph": "madge -i graph.svg --webpack-config webpack.config.js src/apps/Settings.js "
    },
    "dependencies": {
        "dorui": "2",
        "electron-packager": "^15.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.0",
        "@babel/preset-env": "^7.18.2",
        "@csstools/postcss-sass": "^4.0.0",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.1.0",
        "babel-plugin-bynport-directory": "^1.0.0",
        "babel-plugin-import-dir": "^2.0.0",
        "babel-plugin-import-directory": "^1.1.0",
        "babel-plugin-import-recursive": "^1.1.3",
        "babel-plugin-wildcard": "^6.0.0",
        "concurrently": "^5.2.0",
        "copy-webpack-plugin": "^6.0.3",
        "core-js": "^3.36.1",
        "css-loader": "^4.2.0",
        "electron": "^10.1.3",
        "express": "^4.17.1",
        "got": "^11.8.0",
        "open-cli": "^6.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-selector-matches": "^4.0.0",
        "recursive-readdir": "^2.2.2",
        "sass": "^1.26.10",
        "sass-loader": "^10.0.0",
        "style-loader": "^1.2.1",
        "webpack": "^4.44.0",
        "webpack-cli": "^3.3.12"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GameModerator/Electric-Boogaloo"
    },
    "keywords": [
        "electric",
        "boogaloo",
        "thunder",
        "revenge"
    ],
    "author": "Robyn",
    "license": "ISC"
}
